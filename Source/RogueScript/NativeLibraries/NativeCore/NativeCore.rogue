module RogueScript

$include NativeCore/NativePrimitives
$include NativeCore/NativeCompounds

class NativeCore [singleton]
  METHODS
    method register( vm:VM )
      NativePrimitives.register( vm )
      NativeCompounds.register( vm )

      vm.register_method( "System.time_ms()->Int64", () => System.time_ms )
endClass
